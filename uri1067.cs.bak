using System;

namespace uri1067
{
  class Program
  {
    static void Main(string[] args)
    {
		// Read one integer X (1 <= X <= 1000)
		// Print odd number each in one line
		
		int num;
		int count = 1;
		
		num = int.Parse(Console.ReadLine());
		if(num >= 1 && num <= 1000)
		{
			
			if(count <= num && num % count != 0)
			{
				Console.WriteLine(count);
				count++;
			}
			
		}
		
    }
  }
}